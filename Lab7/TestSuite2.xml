<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RegressionTest" parallel="tests">
    <test name="RegressionOnEdge">
        <parameter name="browser" value="Edge"></parameter>
        <classes>
            <class name="Lab7">
                <methods>
                    <include name="TestCase2"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="RegressionOnIE">
        <parameter name="browser" value="IE"></parameter>
        <classes>
            <class name="Lab7">
                <methods>
                    <include name="TestCase2"></include>
                </methods>
            </class>
        </classes>
    </test>


</suite>
